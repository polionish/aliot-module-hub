{
    "source": {
        "include": ["src"],          
        "includePattern": ".+\\.mjs$",
        "excludePattern": "(node_modules/|dist/)"
    },
    "opts": {
        "destination": "./docs",
        "recurse": true,
        "encoding": "utf8",
        "readme": "./README.md"
    },
    "templates": {
        "default": {
            "outputSourceFiles": true
        }
    }
}